{
  "Ruta_TEMP": "TEMP",

  "SchedulerNacDep": {
    "minute": "5, 20, 45"
  },
  "SchedulerMunicipal": {
    "hour" : "1,3,5,7,9,11,13,15,18,21,23",
    "minute": "15,44"
  },
  "SchedulerSNIS": {
    "hour" : "5,13,21,18",
    "minute" : "30,17"
  }
 ,
  "Logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "multi-thread-line": {
        "class": "logging.Formatter",
        "style": "{",
        "datefmt": "%Y-%d-%m %H:%M:%S",
        "format": "{asctime:s} {levelname:8s} [{name}.{funcName}] {message:s}"
      }
    },
    "handlers": {
      "console": {
        "level": "INFO",
        "class": "logging.StreamHandler",
        "formatter": "multi-thread-line",
        "stream": "ext://sys.stdout"
      },
      "file_handler": {
        "level": "INFO",
        "class": "logging.handlers.TimedRotatingFileHandler",
        "formatter": "multi-thread-line",
        "filename": "LoggerConsultaCOVID_19.log",
        "encoding": "utf-8",
		"delay" : true,
        "when": "D",
        "interval": 1,
        "backupCount": 60
      }
    },
    "loggers": {},
    "root": {
      "handlers": [ "console", "file_handler" ],
      "level": "INFO"
    }
  }
}
